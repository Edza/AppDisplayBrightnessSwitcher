<Window x:Class="AppDisplayBrightnessSwitcher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525" Loaded="Window_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Button Grid.Row="0" Content="hide" Click="Button_Click" />
        <Button Grid.Row="1" Content="reset" Click="Button_Click_1" />
        <TextBox Grid.Row="2" Text="122" x:Name="tb"/>
        <TextBox Grid.Row="3" x:Name="tb2"/>
    </Grid>
</Window>
